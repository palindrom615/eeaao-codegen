{{- if index . "$ref" -}}
    {{ title (trimPrefix "#/$defs/" (index . "$ref")) }}
{{- else if eq .type "string" -}}
    string
{{- else if eq .type "integer" -}}
    int
{{- else if eq .type "number" -}}
    float64
{{- else if eq .type "boolean" -}}
    bool
{{- else if eq .type "array" -}}
    []{{ include "type-mapping.tmpl" $.items }}
{{- else if eq .type "object" -}}
    {{ include "struct.tmpl" .properties }}
{{- else -}}
    any
{{- end -}}
